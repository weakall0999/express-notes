generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Notes {
  id        String   @id @unique @default(uuid())
  name      String
  body      String
  create_at DateTime @default(now())
  update_at DateTime @updatedAt
}
